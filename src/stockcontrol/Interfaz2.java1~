        jLabel24.setText(String.valueOf(p.getcantidad()));
        jLabel33.setText(String.valueOf(p.getprecioUnitario()));
        jLabel42.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Cumbia");
