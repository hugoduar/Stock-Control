    private void jMenuItem2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jMenuItem2ActionPerformed
        // TODO add your handling code here:
        jPanel2.setVisible(false);
        jPanel1.setVisible(false);
        jPanel3.setVisible(true);
        jPanel4.setVisible(false);
        Inventario in = new Inventario("datos.txt");
        Producto p = new Producto();
        int pos;
        pos = in.BuscarProducto("Rock Pop");
        p = in.Consulta(pos);
        jLabel17.setText(String.valueOf(p.getcantidad()));
        jLabel26.setText(String.valueOf(p.getprecioUnitario()));
        jLabel35.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Salsa");
        p = in.Consulta(pos);
        jLabel18.setText(String.valueOf(p.getcantidad()));
        jLabel27.setText(String.valueOf(p.getprecioUnitario()));
        jLabel36.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Metal");
        p = in.Consulta(pos);
        jLabel19.setText(String.valueOf(p.getcantidad()));
        jLabel28.setText(String.valueOf(p.getprecioUnitario()));
        jLabel37.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Banda");
        p = in.Consulta(pos);
        jLabel20.setText(String.valueOf(p.getcantidad()));
        jLabel29.setText(String.valueOf(p.getprecioUnitario()));
        jLabel38.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Balada");
        p = in.Consulta(pos);
        jLabel21.setText(String.valueOf(p.getcantidad()));
        jLabel30.setText(String.valueOf(p.getprecioUnitario()));
        jLabel39.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Psycho");
        p = in.Consulta(pos);
        jLabel22.setText(String.valueOf(p.getcantidad()));
        jLabel31.setText(String.valueOf(p.getprecioUnitario()));
