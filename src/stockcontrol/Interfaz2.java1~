        p = in.Consulta(pos);
        jLabel20.setText(String.valueOf(p.getcantidad()));
        jLabel29.setText(String.valueOf(p.getprecioUnitario()));
        jLabel38.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Balada");
        p = in.Consulta(pos);
        jLabel21.setText(String.valueOf(p.getcantidad()));
        jLabel30.setText(String.valueOf(p.getprecioUnitario()));
        jLabel39.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Psycho");
        p = in.Consulta(pos);
        jLabel22.setText(String.valueOf(p.getcantidad()));
        jLabel31.setText(String.valueOf(p.getprecioUnitario()));
        jLabel40.setText(String.valueOf(p.getprecioFinal()));
        
        pos = in.BuscarProducto("Rock Alternativo");
