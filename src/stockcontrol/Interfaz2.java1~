        p = in.Consulta(pos);
        jLabel23.setText(String.valueOf(p.getcantidad()));
        jLabel32.setText(String.valueOf(p.getprecioUnitario()));
        jLabel41.setText(String.valueOf(p.getprecioFinal()));
        
